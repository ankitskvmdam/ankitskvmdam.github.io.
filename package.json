{
  "name": "ankitskvmdam.github.io.",
  "version": "1.0.0",
  "description": "This is my personal website",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --progress -d --config webpack.config.js --watch --watch-poll",
    "webpackBuild": "webpack -p --progress --config webpack.config.js",
    "hugoServer": "cd ankdev && hugo server -D",
    "hugoBuild": "cd ankdev && hugo",
    "dev": "concurrently --prefix '{time}-{pid}' 'npm run watch' 'npm run hugoServer' --kill-others --kill-others-on-fail",
    "copy_project": "bash add_project_webpages.sh",
    "build": "npm run webpackBuild && npm run hugoBuild && npm run copy_project",
    "deploy": "npm run build && gh-pages -d ./ankdev/public",
    "ghpages": "gh-pages -d ./ankdev/public"
  },
  "author": "Ankit Kumar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ankitskvmdam/ankitskvmdam.github.io./issues"
  },
  "homepage": "https://github.com/ankitskvmdam/ankitskvmdam.github.io.#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^8.6.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "extract-css-chunks-webpack-plugin": "^3.2.1",
    "file-loader": "^1.1.11",
    "gh-pages": "^2.1.1",
    "js-yaml": "^3.13.1",
    "lost": "^8.2.1",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "rucksack-css": "^1.0.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "sugarss": "^1.0.1",
    "toml": "^2.3.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2",
